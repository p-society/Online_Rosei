{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
      "object-literal-sort-keys": [false],
      "ter-indent": [true, 2],
      "no-console": false,
      "only-arrow-functions": [false],
      "max-classes-per-file": [true, 5],
      "no-shadowed-variable": false,
      "interface-name": [false]
    },
    "rulesDirectory": []
}
